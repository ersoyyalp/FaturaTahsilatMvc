<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" />

</head>
<body style="background-color:#82c0c2">

    <nav class="navbar navbar-expand-lg navbar-dark" style="background-color:#0d8fa9;">

        <div class="container-fluid">
            <a class="navbar-brand" asp-action="Index" asp-controller="Home">Fatura Tahsilat</a>
         
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-end" id="navbarNav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a class="nav-link active" asp-action="Sepet" asp-controller="Home" aria-current="page">Ödenecek Faturalarım</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link active" asp-action="SignOut" asp-controller="Home" aria-current="page">Çıkış Yap</a>
                    </li>

                </ul>
            </div>
        </div>

    </nav>
    <div class="mt-2">
        <div class="container-fluid">
          
            @RenderBody()
        </div>





    </div>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css" integrity="sha512-1sCRPdkRXhBV2PBLUdRb4tMg1w2YPf37qatUFeS7zlBy7jJI8Lf4VHwWfZZfpXtYSLy85pkm9GaYVYMfw5BC1A==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <script src="~/jquery/jquery.min.js"></script>
    <script src="~/twitter-bootstrap/js/bootstrap.min.js"></script>
    @RenderSection("JavaScript", false)
</body>
</html>
